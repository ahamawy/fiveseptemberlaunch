{
  "name": "investor-portal-dashboard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "typecheck": "tsc --noEmit",
    "ci:check": "npm run typecheck && npm run lint && npm run test && npm run test:e2e -- --reporter=list",
    "setup": "node scripts/setup.js",
    "setup:env": "cp .env.example .env.local 2>/dev/null || cp .env.development .env.local",
    "setup:db": "echo 'Database setup: See DB/migrations/ for Supabase setup'",
    "clean": "rm -rf .next node_modules",
    "clean:install": "npm run clean && npm install",
    "db:drizzle:introspect": "drizzle-kit introspect --config=drizzle.config.ts",
    "db:drizzle:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "db:migrate:up": "tsx scripts/db-migrate.ts up",
    "db:migrate:status": "tsx scripts/db-migrate.ts status",
    "db:seed": "tsx scripts/seed-fee-schedule-config.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:mcp": "docker-compose up mcp-supabase mcp-equitie -d",
    "docker:migrate": "docker-compose --profile migrate up migration-runner"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.1",
    "@heroicons/react": "^2.2.0",
    "@nivo/bar": "^0.85.0",
    "@nivo/core": "^0.85.0",
    "@nivo/line": "^0.85.0",
    "@nivo/pie": "^0.85.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tailwindcss/forms": "^0.5.10",
    "chart.js": "^4.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.4",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.539.0",
    "multer": "^2.0.2",
    "next": "14.2.5",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "tesseract.js": "^6.0.1",
    "victory": "^36.9.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.14.10",
    "@types/node-fetch": "^2.6.13",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "msw": "^2.3.0",
    "pg": "^8.11.5",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3",
    "vite": "^5.3.0",
    "vitest": "^1.6.0",
    "zod": "^3.25.76"
  }
}
